<mvc:View
	height="100%"
	controllerName="sap.m.sample.ComboBox.controller.ComboBox"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page showHeader="false" class="sapUiContentPadding">
		<content>
			<ComboBox
				items="{
					path: 'CC>/CountriesCollection'
				}">
				<core:Item text="{CC>text}" />
			</ComboBox>
		</content>
	</Page>
</mvc:View>